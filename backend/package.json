{
  "name": "redis-ai-trading-bot-backend",
  "version": "1.0.0",
  "description": "Backend service for RSI Divergence Trading Bot",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:divergence": "jest --testPathPattern=strategies/divergence",
    "test:risk": "jest --testPathPattern=services/risk",
    "test:volume": "jest --testPathPattern=strategies/volume",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.10",
    "technicalindicators": "^3.1.0",
    "tulind": "^0.8.20",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ]
  },
  "keywords": [
    "trading",
    "rsi",
    "divergence",
    "redis",
    "ai",
    "risk-management"
  ],
  "author": "Trading Bot Developer",
  "license": "MIT"
}
